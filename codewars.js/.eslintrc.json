{
    "env": {
        "browser": true,
        "es2021": true,
        "jest/globals": true
    },
    "extends": "eslint:recommended",
    "plugins": ["jest"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "ignorePatterns": ["dist/", "node_modules/"],
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["warn", "single"]
    }
}
